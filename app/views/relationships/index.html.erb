<% if @relatives.empty? %>

    <%= render partial: 'shared/nothing_to_render_alert', locals: { item: 'relative' } %>

<% else %>

    <ul id="relatives" class="list-group">

      <% @relatives.each do |relative| %>

        <%= render partial: 'relative', locals: { relative: relative } %>

      <% end %>

    </ul>

    <%= paginate @relatives %>

<% end %>









