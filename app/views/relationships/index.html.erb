<% if @relatives.empty? %>

    <%= render partial: 'shared/nothing_to_render_alert', locals: { item: 'relative' } %>

<% else %>

    <ul class="list-group">

      <% @relatives.each do |relative| %>

          <%= render partial: 'relationship', locals: { item: relative }  %>

      <% end %>

    </ul>

    <%= paginate @relatives %>

<% end %>









